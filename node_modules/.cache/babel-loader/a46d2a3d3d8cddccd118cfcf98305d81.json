crB,IAA1B,CAAN;AACAU,QAAAA,MAAM,CAAC,WAAD,EAAclB,KAAd,CAAN;AACAsB,QAAAA,OAAO,CAACO,IAAD,CAAP;AACD,OAlBD,EAkBGF,KAlBH,CAkBS,UAAAC,KAAK,EAAI;AAChBL,QAAAA,MAAM,CAACK,KAAD,CAAN;AACD,OApBD;AAqBD,KAtBM,CAAP;AAuBD,GAxCa;AA0Cd;AACMI,EAAAA,UA3CQ,6BA2CenB,QA3Cf,EA2CyB;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAApBK,cAAAA,MAAoB,SAApBA,MAAoB;AAAA;AAAA,qBAChB3B,aAAa,CAACsB,QAAD,CADG;;AAAA;AAC/BoB,cAAAA,MAD+B;;AAAA,oBAEjCA,MAAM,CAACC,IAAP,IAAe,GAFkB;AAAA;AAAA;AAAA;;AAAA,gDAI5BD,MAJ4B;;AAAA;AAAA,gDAO5BA,MAP4B;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAStC,GApDa;AAsDd;AACAE,EAAAA,MAvDc,yBAuDY;AAAA,QAAjBjB,MAAiB,SAAjBA,MAAiB;AAAA,QAAThB,KAAS,SAATA,KAAS;AACxB,WAAO,IAAImB,OAAJ,CAAY,UAACC,OAAD,EAAUC,MAAV,EAAqB;AACtC5B,MAAAA,WAAW,GAD2B,CACxB;;AACdC,MAAAA,WAAW;AACXsB,MAAAA,MAAM,CAAC,aAAD,CAAN;AACAI,MAAAA,OAAO;AACR,KALM,CAAP;AAMD,GA9Da;AAgEd;AACAc,EAAAA,UAjEc,6BAiES;AAAA,QAAVlB,MAAU,SAAVA,MAAU;AACrB,WAAO,IAAIG,OAAJ,CAAY,UAAAC,OAAO,EAAI;AAC5B3B,MAAAA,WAAW,GADiB,CACd;;AACduB,MAAAA,MAAM,CAAC,aAAD,CAAN;AACAI,MAAAA,OAAO;AACR,KAJM,CAAP;AAKD,GAvEa;AAyEd;AACMe,EAAAA,UA1EQ,6BA0Ee7B,IA1Ef,EA0EqB;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAhBU,cAAAA,MAAgB,SAAhBA,MAAgB;AAAA;AAAA,qBACZ1B,aAAa,CAAC;AAAEgB,gBAAAA,IAAI,EAAEA;AAAR,eAAD,CADD;;AAAA;AAC3ByB,cAAAA,MAD2B;;AAAA,oBAE7BA,MAAM,CAACC,IAAP,KAAgB,GAFa;AAAA;AAAA;AAAA;;AAAA,gDAGxBD,MAAM,CAACJ,IAHiB;;AAAA;AAAA,gDAKxBI,MAAM,CAACK,OALiB;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAOlC;AAjFa,CAAhB;AAqFA,eAAe;AACbC,EAAAA,UAAU,EAAE,IADC;AAEbrC,EAAAA,KAAK,EAALA,KAFa;AAGbC,EAAAA,SAAS,EAATA,SAHa;AAIbc,EAAAA,OAAO,EAAPA;AAJa,CAAf","sourcesContent":["// import {  logout } from '@/api/user'\nimport { login, getInfo, reqUpdateInfo, reqGetMyApply } from '@/api'\nimport { getToken, setToken, removeToken } from '@/utils/auth'\nimport { resetRouter } from '@/router'\n// import { response } from 'express'\n// import { reject } from 'core-js/fn/promise'\n\nconst getDefaultState = () => {\n  return {\n    token: getToken(),\n    name: '',\n    roles: [],\n    avatar: 'https://wpimg.wallstcn.com/f778738c-e4f8-4870-b634-56703b4acafe.gif'\n  }\n}\n\nconst state = getDefaultState()\n\nconst mutations = {\n  RESET_STATE: (state) => {\n    Object.assign(state, getDefaultState())\n  },\n  SET_UID: (state, u_id) => {\n    state.u_id = u_id\n  },\n  SET_TOKEN: (state, token) => {\n    state.token = token\n  },\n  SET_NAME: (state, username) => {\n    state.name = username\n  },\n  SET_INFO: (state, userInfo) => {\n    state.userInfo = userInfo\n  },\n  SET_MYAPPLY: (state, myapply) => {\n    state.myapply = myapply\n  },\n  SET_ROLES: (state, roles) => {\n    state.roles = roles\n  }\n}\n\nconst actions = {\n  // user login\n  async login({ commit, state }, userInfo) {\n    const { username, password, role } = userInfo\n    return await new Promise((resolve, reject) => {\n      login({ username: username.trim(), password: password, role: role }).then(response => {\n        commit('SET_TOKEN', response.token)\n        setToken(response.token)\n        resolve()\n      }).catch(error => {\n        reject(error)\n      })\n    })\n  },\n\n  // get user info\n  getInfo({ commit, state }) {\n    return new Promise((resolve, reject) => {\n      getInfo(state.token).then(response => {\n        const data = response.data\n        if (!data) {\n          return reject('Verification failed, please Login again.')\n        }\n        const username = data.u_name\n        commit('SET_INFO', data)\n        commit('SET_NAME', username)\n        const roles = []\n        if (response.data.u_id == 1) {\n          roles.push('editor')\n        } else {\n          roles.push('admin')\n        }\n\n        commit('SET_UID', response.data.u_id)\n        commit('SET_ROLES', roles)\n        resolve(data)\n      }).catch(error => {\n        reject(error)\n      })\n    })\n  },\n\n  // 个人信息更新\n  async updateInfo({ commit }, userInfo) {\n    const result = await reqUpdateInfo(userInfo)\n    if (result.code == 200) {\n      // 更新成功的响应\n      return result\n    } else {\n      // 更新失败的响应\n      return result\n    }\n  },\n\n  // user logout\n  logout({ commit, state }) {\n    return new Promise((resolve, reject) => {\n      removeToken() // must remove  token  first\n      resetRouter()\n      commit('RESET_STATE')\n      resolve()\n    })\n  },\n\n  // remove token\n  resetToken({ commit }) {\n    return new Promise(resolve => {\n      removeToken() // must remove  token  first\n      commit('RESET_STATE')\n      resolve()\n    })\n  },\n\n  // 获取用户的申请记录\n  async getMyapply({ commit }, u_id) {\n    const result = await reqGetMyApply({ u_id: u_id })\n    if (result.code === 200) {\n      return result.data\n    } else {\n      return result.message\n    }\n  }\n\n}\n\nexport default {\n  namespaced: true,\n  state,\n  mutations,\n  actions\n}\n\n"]}]}