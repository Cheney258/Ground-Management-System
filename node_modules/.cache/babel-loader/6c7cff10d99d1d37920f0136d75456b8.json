<template>\r\n  <div>\r\n    <el-card>\r\n      <div slot=\"header\">\r\n        <el-select v-model=\"value\" @change=\"change\" clearable placeholder=\"请选择赛事类型\" style=\"margin-left:250px;width:150px\">\r\n          <el-option\r\n            v-for=\"(item,index) in gameType\"\r\n            :key=\"index\"\r\n            :label=\"item\"\r\n            :value=\"item\">\r\n          </el-option>\r\n        </el-select>\r\n      </div>\r\n      <div>\r\n        <el-table :data=\"showList\" :default-sort = \"{prop: 'gametime', order: 'ascending'}\" style=\"width: 100%\">\r\n          <el-table-column prop=\"gid\" sortable label=\"赛事编号\" width=\"width\" align=\"center\">\r\n          </el-table-column>\r\n          <el-table-column prop=\"gametype\" label=\"赛事类型\" width=\"width\" align=\"center\">\r\n          </el-table-column>\r\n          <el-table-column prop=\"gamename\" label=\"赛事名称\" width=\"width\" align=\"center\">\r\n          </el-table-column>\r\n          <el-table-column prop=\"class\" label=\"比赛队伍\" width=\"width\" align=\"center\">\r\n          </el-table-column>\r\n          <el-table-column prop=\"gametime\" sortable label=\"比赛时间\" width=\"width\" align=\"center\">\r\n          </el-table-column>\r\n          <el-table-column prop=\"position\" label=\"比赛地点\" width=\"width\" align=\"center\">\r\n          </el-table-column>\r\n        </el-table>\r\n      </div>\r\n    </el-card>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: 'Gquery',\r\n\r\n  data() {\r\n    return {\r\n      value:'',\r\n      gameType:[],\r\n      showList:[],\r\n      gameList:[]\r\n    }\r\n  },\r\n\r\n  mounted() {\r\n    this.getGameList()\r\n  },\r\n\r\n  methods: {\r\n    // 获取全部赛事\r\n    async getGameList(){\r\n      const results = await this.$store.dispatch('getGameList')\r\n      if(results.code == 200){\r\n        this.$notify.success({\r\n          title:'成功',\r\n          message:results.message\r\n        })\r\n        this.gameList = results.data\r\n        this.gameList.forEach(item => {\r\n          item.gametime = this.$moment(item.gametime).format(\"YYYY-MM-DD HH:mm:ss\");\r\n          this.gameType.push(item.gametype)\r\n        });\r\n        this.showList = this.gameList\r\n      }else{\r\n        this.$notify.error({\r\n          title:'失败',\r\n          message:results.message\r\n        })\r\n      }\r\n      // console.log(this.gameList)\r\n      // console.log(this.gameType)\r\n    },\r\n\r\n    //按类型展示\r\n    change(){\r\n      \r\n      if(this.value == ''){\r\n        this.showList = this.gameList\r\n      }else{\r\n        this.showList=[]\r\n        this.gameList.forEach(item => {\r\n          if(item.gametype == this.value){\r\n            this.showList.push(item)\r\n          }\r\n        })\r\n      }\r\n      \r\n\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n\r\n</style>\r\n"]}]}