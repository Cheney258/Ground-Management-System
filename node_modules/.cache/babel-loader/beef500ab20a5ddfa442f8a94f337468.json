D,CAAN;AACD,iBARD;AASD,eAVY,CADkB;;AAAA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAYhC,GAzEa;AA2Ed;AACMM,EAAAA,MA5EQ,yBA4EQC,KA5ER,EA4Ec;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAdvB,cAAAA,MAAc,SAAdA,MAAc;AAAA;AAAA,qBACb,IAAIM,OAAJ,CAAY,UAACC,OAAD,EAASC,MAAT,EAAoB;AAC3CZ,gBAAAA,cAAc,CAAC2B,KAAD,CAAd,CAAsBd,IAAtB,CAA2B,UAAAC,GAAG,EAAI;AAChCH,kBAAAA,OAAO,CAACG,GAAD,CAAP;AACD,iBAFD,EAEGC,KAFH,CAES,UAAAK,GAAG,EAAI;AACdR,kBAAAA,MAAM,CAACQ,GAAD,CAAN;AACD,iBAJD;AAKD,eANY,CADa;;AAAA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAQ3B;AApFa,CAAhB;AAuFA,IAAMQ,SAAS,GAAG,EAAlB;AAEA,IAAMC,OAAO,GAAG,EAAhB;AAEA,eAAe;AACb5B,EAAAA,KAAK,EAALA,KADa;AAEbC,EAAAA,OAAO,EAAPA,OAFa;AAGb0B,EAAAA,SAAS,EAATA,SAHa;AAIbC,EAAAA,OAAO,EAAPA;AAJa,CAAf","sourcesContent":["import { reqGetVenueInfo,reqCheckCode,reqGetVenueType,\r\n  reqSubVenueApply,reqGetPosition,reqGetVenueApply,reqCancelApply } from \"@/api\"\r\n\r\n\r\n\r\nconst state = {}\r\n\r\nconst actions = {\r\n  // 获取场地信息\r\n  async getvenueinfo({commit}){\r\n    let result =await reqGetVenueInfo()\r\n    const venueInfo = result.data\r\n    return venueInfo\r\n  },\r\n\r\n  // 审查审批号，获取活动申请信息去申请场地\r\n  async checkcode({commit},code){\r\n    return new Promise((resolve, reject) => {\r\n      reqCheckCode({code:code}).then(res => {\r\n        if(res.code == 0){\r\n          resolve(res)\r\n        }else{\r\n          resolve()\r\n        }\r\n      }).catch(error => {\r\n        reject(error)\r\n      })\r\n    })\r\n  },\r\n\r\n  // 获取场地类型\r\n  async getVenueType({commit}){\r\n    return await new Promise((resolve,reject) => {\r\n      // console.log(11)\r\n      reqGetVenueType().then(res => {\r\n        resolve(res.data)\r\n      }).catch(error => {{\r\n        reject(error)\r\n      }})\r\n    })\r\n  },\r\n\r\n  // 获取对应类型场地位置\r\n  async getFieldList({commit},tid){\r\n    return await new Promise((resolve,reject) => {\r\n      reqGetPosition({tid:tid}).then(res => {\r\n        // console.log(res)\r\n        resolve(res)\r\n      }).catch(err =>{\r\n        reject(err)\r\n      })\r\n    })\r\n  },\r\n\r\n  // 提交用户场地申请\r\n  async submitApply({commit},params){\r\n    return await new Promise((resolve,reject) => {\r\n      // console.log(params)\r\n      reqSubVenueApply(params).then(res => {\r\n        resolve(res)\r\n      }).catch(error => {\r\n        reject(error)\r\n      })\r\n    })\r\n  },\r\n\r\n  \r\n  // 获取用户场地申请记录\r\n  async getVenueApply({commit},uid){\r\n    return await new Promise((resolve,reject) => {\r\n      reqGetVenueApply({ uid: uid }).then(res => {\r\n        if(res.code == 200){\r\n          resolve(res.data)\r\n        } else {\r\n          resolve(res.message)\r\n        }\r\n      }).catch(err => {\r\n        reject(err)\r\n      })\r\n    })\r\n  },\r\n\r\n  // 取消用户申请\r\n  async cancel({commit},param){\r\n    return await new Promise((resolve,reject) => {\r\n      reqCancelApply(param).then(res => {\r\n        resolve(res)\r\n      }).catch(err => {\r\n        reject(err)\r\n      })\r\n    })\r\n  }\r\n}\r\n\r\nconst mutations = {}\r\n\r\nconst getters = {}\r\n\r\nexport default {\r\n  state,\r\n  actions,\r\n  mutations,\r\n  getters\r\n}"]}]}