ACA,KA9BA;AA+BA;AACAC,gBAhCA,0BAgCA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBACA,sCADA;;AAAA;AACAvB,oBADA;AAEA,iDAFA,CAGA;;AAHA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAIA,KApCA;AAqCA;AACAwB,mBAtCA,6BAsCA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AACA;AACA;AACA;AAHA;AAAA,uBAIA,gEAJA;;AAAA;AAIAL,sBAJA;;AAKA;AACA;AACAM;AACA,iBAHA,MAGA;AACA;AACA;;AAVA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAWA,KAjDA;AAkDA;AACAC,UAnDA,kBAmDAC,IAnDA,EAmDA;AAAA;;AACA;AACA;AACA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,yBACAC,KADA;AAAA;AAAA;AAAA;;AAEAC,0BAFA,GAEA;AACAC,oDADA;AAEAC,6CAFA;AAGAC,mDAHA;AAIAC,mDAJA;AAKAC,uDALA;AAMAxB,sGANA;AAOAL;AAPA,qBAFA,EAWA;;AAXA;AAAA;AAAA,2BAaA,6CAbA;;AAAA;AAaAc,0BAbA;;AAcA;AACA;;AACA;AACA;AACA,qBAJA,MAIA;AACA;AACA;;AApBA;AAAA;;AAAA;AAAA;AAAA;;AAsBA;;AAtBA;AAAA;AAAA;;AAAA;AA0BA;;AA1BA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;;AAAA;AAAA;AAAA;AAAA;AA6BA,OA9BA,MA8BA;AACA;AACAC,uBADA;AAEAN,+CAFA;AAGAO;AAHA;AAKA;AAGA;AA5FA;AAnCA","names":["name","data","isShow","ifCheckCode","active","appinfo","code","field","fieldClass","filedType","filedname","apptime","usetime","rules","required","message","trigger","mounted","methods","check","result","title","offset","resetting","getVenueType","getPositionList","console","submit","form","valid","params","uid","gid","tid","pid","gametime"],"sourceRoot":"src/views/users/apply","sources":["field.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <el-card>\r\n      <div slot=\"header\">\r\n        <!-- 步骤条 -->\r\n        <el-steps :active=\"active\"   finish-status=\"success\" align-center >\r\n          <el-step title=\"填写申请\" ></el-step>\r\n          <el-step title=\"等待审核\" ></el-step>\r\n          <el-step title=\"等待结果\" ></el-step>\r\n        </el-steps>\r\n      </div>\r\n      <div v-if=\"active == 1\" style=\"width:500px;margin:50px auto\">\r\n        <!-- 表单 -->\r\n    <el-form ref=\"form\" label-width=\"200\" :model=\"appinfo\" :rules=\"rules\" size=\"medium\">\r\n      <!-- 审批号 -->\r\n      <el-form-item label=\"审批号：\"  prop=\"code\">\r\n        <el-input v-model=\"appinfo.code\" style=\"width:150px\" placeholder=\"请输入活动审批号\"></el-input>\r\n        <el-button type=\"primary\"  style=\"margin-left:10px\" @click=\"check\">Check</el-button>\r\n        <h5 v-show=\"!ifCheckCode\" v-if=\"isShow\" style=\"margin:0 0 0 65px;color:red;\">（请先输入审批号以继续场地申请）</h5>\r\n        <h5 v-show=\"ifCheckCode\" style=\"margin:0 0 0 65px;color:green;\">验证通过，请继续输入信息</h5>\r\n      </el-form-item>\r\n\r\n      <el-form-item label=\"活动时间：\" style=\"margin-top:5px\">\r\n        <span v-show=\"ifCheckCode\" class=\"appInfo\">{{appinfo.gametime}}</span>\r\n      </el-form-item>\r\n\r\n      <el-form-item label=\"申请人姓名：\">\r\n        <span v-show=\"ifCheckCode\" class=\"appInfo\">{{appinfo.username}}</span>\r\n      </el-form-item>\r\n\r\n      <el-form-item label=\"申请活动类型：\">\r\n        <span v-show=\"ifCheckCode\" class=\"appInfo\">{{appinfo.gametype}}</span>\r\n      </el-form-item>\r\n\r\n      <el-form-item label=\"场地类型：\" prop=\"fieldClass\">\r\n        <el-select v-model=\"appinfo.filedType\"  v-show=\"ifCheckCode\" @change=\"getPositionList\"  placeholder=\"请选择场地类型\" style=\"width:400px\">\r\n          <el-option\r\n            v-for=\"(item,index) in appinfo.fieldClass\"\r\n            :key=\"index\"\r\n            :label=\"item.type\"\r\n            :value=\"item.tid\">         \r\n          </el-option>\r\n        </el-select>\r\n      </el-form-item> \r\n      <el-form-item label=\"选择场地：\" prop=\"field\">\r\n        <el-select v-model=\"appinfo.filedname\"  v-show=\"ifCheckCode\"  placeholder=\"请选择场地\" style=\"width:400px\">\r\n        <el-option\r\n          v-for=\"(item,index) in appinfo.field\"\r\n          :key=\"index\"\r\n          :label=\"item.position\"\r\n          :value=\"item.pid\">\r\n          \r\n        </el-option>\r\n      </el-select>\r\n      </el-form-item>\r\n\r\n      <!-- <el-form-item label=\"使用时间段：\" prop=\"usetime\">\r\n        <el-select v-model=\"model\"   v-show=\"ifCheckCode\"  placeholder=\"可使用时间段\">\r\n          <el-option label=\"label\" value=\"value\"></el-option>\r\n        </el-select>\r\n      </el-form-item> -->\r\n      \r\n      <el-form-item >\r\n          <el-button type=\"primary\" @click=\"submit('from')\">下一步</el-button>\r\n          <el-button type=\"warning\" @click=\"resetting\">重置</el-button>\r\n      </el-form-item>\r\n\r\n    </el-form>\r\n      </div>\r\n\r\n      <!-- 提交成功u页面 -->\r\n      <div v-if=\"active == 2\">\r\n            <div class=\"success\">\r\n                <img src=\".//success.png\" class=\"successIcon\"/><br>\r\n                <div style=\"font-size:20px;margin-top:5px\">提交成功</div>\r\n                <div style=\"margin-top:25px;\">审批号为：<span class=\"titleInfo\" >{{this.appinfo.code}}</span>的场地申请已提交，待场地管理员审批后即可完成场地申请</div>\r\n            </div>\r\n        </div>\r\n        <div class=\"content\" v-if=\"active == 2\">\r\n            <div class=\"content_header\">\r\n                <span class=\"title\">审批号：</span><span class=\"titleInfo\">{{appinfo.code}}</span>\r\n                <span class=\"title\">申请人：</span><span class=\"titleInfo\">{{appinfo.username}}</span>\r\n                <!-- <span>申请日期：</span> -->\r\n            </div>\r\n            <div style=\"height: 300px;\"  class=\"step\">\r\n              <el-steps direction=\"vertical\" :active=\"3\">\r\n                <el-step title=\"创建申请\" :description=\"appinfo.apptime\"></el-step>\r\n                <el-step title=\"赛事审批\" description=\"\"></el-step>\r\n                <el-step title=\"场地审批\" description=\"\"></el-step>\r\n                <el-step title=\"完成申请\" description=\"\"></el-step>\r\n              </el-steps>\r\n            </div>\r\n        </div>\r\n    </el-card>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: 'Field',\r\n\r\n  data() {\r\n    return {\r\n      isShow:true,\r\n      ifCheckCode:false,\r\n      active:1,\r\n      appinfo:{\r\n        code:'',\r\n        field: [],\r\n        fieldClass: [],\r\n        filedType:'',\r\n        filedname:'',\r\n        apptime:'',\r\n        usetime:''\r\n      },\r\n      \r\n      \r\n      rules:{\r\n        code:[{required:true,message:'必填项',trigger: 'blur'}],\r\n        usetime:[{required:true,message:'必填项', trigger: 'blur'}],\r\n        // username:[{required:true,message:'必填项', trigger: 'blur'}],\r\n        // gametype:[{required:true,message:'必填项', trigger: 'blur'}],\r\n        fieldClass:[{required:true,message:'必填项', trigger: 'blur'}],\r\n        field:[{required:true,message:'必填项', trigger: 'blur'}]\r\n      }\r\n\r\n    }\r\n  },\r\n\r\n  mounted() {\r\n    this.getVenueType()\r\n  },\r\n\r\n  methods: {\r\n    // 审查审批号\r\n    async check(){\r\n      const result = await  this.$store.dispatch('checkcode', this.appinfo.code)\r\n      if(result){\r\n        if(result.code == 200 ){     \r\n          this.ifCheckCode = true\r\n          this.appinfo.gid = result.data.gid\r\n          this.appinfo.code = result.data.code\r\n          this.appinfo.gametime = this.$moment(result.data.gametime).format('YYYY-MM-DD HH:mm:ss')\r\n                this.appinfo.username = this.$store.getters.name\r\n          this.appinfo.gametype = result.data.gametype\r\n        }else{\r\n          this.$message.error(result.message)\r\n        }\r\n      }else{\r\n        this.isShow = false\r\n        this.$notify.warning({\r\n          title:'警告',\r\n          message:'审批号已被使用',\r\n          offset:100\r\n        })\r\n      }\r\n      \r\n    },\r\n    // 重置\r\n    resetting(){\r\n      this.$refs.form.resetFields()\r\n      this.ifCheckCode = false\r\n      this.isShow = true\r\n    },\r\n    // 获取场地类型和\r\n    async getVenueType(){\r\n      const data = await this.$store.dispatch('getVenueType')\r\n      this.appinfo.fieldClass = data\r\n      // console.log(this.appinfo.fieldClass)\r\n    },\r\n    // 获取对应类型的场地地点\r\n    async getPositionList(){\r\n      // console.log('11')\r\n      // 每次重新选择欠先清除二级选择器\r\n      this.appinfo.field = []\r\n      let result = await this.$store.dispatch('getFieldList',this.appinfo.filedType)\r\n      if(result.code == 200) {\r\n        this.appinfo.field = result.data\r\n        console.log(this.appinfo.field)\r\n      }else{\r\n        this.$message.error(result.message)\r\n      }\r\n    },\r\n    // 提交申请\r\n    submit(form){\r\n      // 验证账号是否正常\r\n      if(this.$store.getters.credit == 1){\r\n          this.$refs.form.validate(async (valid) => {\r\n          if(valid){\r\n            const params ={\r\n              uid:this.$store.getters.uid,\r\n              gid:this.appinfo.gid,\r\n              tid:this.appinfo.filedType,\r\n              pid:this.appinfo.filedname,\r\n              gametime:this.appinfo.gametime,\r\n              apptime:this.appinfo.apptime = this.$moment().format('YYYY-MM-DD HH:mm:ss'),\r\n              code:this.appinfo.code,\r\n            }\r\n              // console.log(params)\r\n            try {\r\n              let result = await this.$store.dispatch('submitApply',params)\r\n              if(result.code == 200) {\r\n                this.$message.success('提交成功，等待审核')\r\n                this.active = 2\r\n                this.isShow = true\r\n              }else{\r\n                this.$message.error('提交失败')\r\n              }\r\n            } catch (error) {\r\n              this.$message.error(error)\r\n            }\r\n          \r\n          }else{\r\n            this.message.error('请正确填写表格')\r\n          }\r\n        })\r\n      }else{\r\n        this.$notify.warning({\r\n          title: '账号异常',\r\n          message: '禁止申请，您有过期未归还器材！若有疑问请联系管理员！',\r\n          offset: 100\r\n        })\r\n      }\r\n      \r\n      \r\n    }\r\n  },\r\n  \r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n.active{\r\n  color:red;\r\n}\r\n.appRecordCode {\r\n  color:red;\r\n  margin-left: 10px;\r\n  font-size:23px;\r\n  font-weight: bold;\r\n}\r\n\r\n.appInfo {\r\n  font-size:16px;\r\n  color:#606266;\r\n  font-weight:bold;\r\n  margin-top:2px;\r\n  margin-left: 10px;\r\n}\r\n\r\n.arrow-text {\r\n    font-size:14px;\r\n}\r\n\r\n.btn{\r\n  position: relative;\r\n  margin-top: 50px;\r\n  right: inherit;\r\n  \r\n}\r\n.success{ \r\n    text-align: center;\r\n    // margin-right: 18%;\r\n    margin: 25px auto 0px;\r\n}\r\n\r\n.successIcon {\r\n    width: 70px;\r\n    height:70px;\r\n\r\n}\r\n\r\n.content_header {\r\n    text-align: center;\r\n    position: relative;\r\n    top: 20px;\r\n    margin-top:50px;\r\n}\r\n.content{\r\n    margin:10px auto;\r\n    width: 50%;\r\n    background-color: #F5F5F5;\r\n}\r\n\r\n.title{\r\n    padding-top: 20px;\r\n    font-size: 15px;\r\n    font-weight: bold;\r\n}\r\n\r\n.titleInfo{\r\n    color:rgb(245, 9, 9);\r\n    font-size: 24px;\r\n    font-weight: bold;\r\n    margin-top: 20px;\r\n    margin-right:30px;\r\n    font-size:15px;\r\n}\r\n.step{\r\n  margin: 80px 350px;\r\n}\r\n</style>\r\n"]}]}