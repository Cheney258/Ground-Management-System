em\"\r\n            :value=\"item\">\r\n          </el-option>\r\n        </el-select>\r\n        <el-input v-model=\"search\" placeholder=\"输入关键字搜索\" class=\"search\" style=\"\"></el-input>\r\n        <el-button type=\"primary\">搜索</el-button>\r\n      </div>\r\n      <div>\r\n        <el-table :data=\"showList\" style=\"width: 100%\">\r\n          <el-table-column prop=\"pid\" label=\"场地编号\" width=\"width\" align=\"center\">\r\n          </el-table-column>\r\n          <el-table-column prop=\"type\" label=\"场地类别\" width=\"width\" align=\"center\">\r\n          </el-table-column>\r\n          <el-table-column prop=\"position\" label=\"场地位置\" width=\"width\" align=\"center\">\r\n          </el-table-column>\r\n          <el-table-column prop=\"usetime\" label=\"可预约时间\" width=\"width\" align=\"center\">\r\n          </el-table-column>\r\n          <el-table-column prop=\"status\" label=\"运营状态\" width=\"width\" align=\"center\">\r\n          </el-table-column>\r\n        </el-table>\r\n      </div>\r\n    </el-card>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: 'Fquery',\r\n\r\n  data() {\r\n    return {\r\n      select:'',\r\n      search:'',\r\n      filedType:[],\r\n      filedList:[],\r\n      showList:[]\r\n    }\r\n  },\r\n\r\n  mounted() {\r\n    this.getVenueInfo()\r\n  },\r\n\r\n  methods: {\r\n    // 获取场地信息\r\n    async getVenueInfo(){\r\n      let results = await this.$store.dispatch('getvenueinfo')\r\n      const that = this\r\n      this.filedList = results\r\n      this.filedList.forEach(item => {\r\n        // console.log(that.filedType)\r\n        that.filedType.push(item.type)\r\n        item.status = item.status == 0? '运行中': '暂停使用'\r\n      });\r\n      // 去重\r\n      this.filedType = [...new Set(this.filedType)]\r\n      this.showList = this.filedList\r\n    },\r\n\r\n    change(){\r\n      // console.log(this.select)\r\n      if(this.select == ''){\r\n        this.showList = this.filedList\r\n      }else{\r\n        this.showList = []\r\n        this.filedList.forEach(item => {\r\n          if(this.select == item.type){\r\n            this.showList.push(item)\r\n          }\r\n        })\r\n      }\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n.search{\r\n  display:inline-block;\r\n  width:150px;\r\n  margin: 0 50px;\r\n  border-radius: 20px;\r\n}\r\n</style>\r\n"]}]}