           :key=\"index\"\r\n            :label=\"item\"\r\n            :value=\"item\">\r\n          </el-option>\r\n        </el-select>\r\n        <el-input v-model=\"search\" placeholder=\"关键字搜索\" class=\"input\"></el-input>\r\n        <el-button type=\"primary\" class=\"btn\">搜索</el-button>\r\n      </div>\r\n      <div>\r\n        <el-table :data=\"showList\" style=\"width: 100%;margin:10px\">\r\n          <el-table-column prop=\"eid\" label=\"器材ID\" width=\"80\" fixed align=\"center\">\r\n          </el-table-column>\r\n          <el-table-column prop=\"type\" label=\"器材类型\" width=\"width\" align=\"center\">\r\n          </el-table-column>\r\n          <el-table-column prop=\"equipname\" label=\"器材名称\" width=\"width\" align=\"center\">\r\n          </el-table-column>\r\n          <el-table-column prop=\"status\" label=\"器材状态\" width=\"width\" align=\"center\">\r\n            <template slot-scope=\"scope\">\r\n              <el-tag type=\"success\" v-if=\"scope.row.status == 0\" >正常</el-tag>\r\n              <el-tag type=\"warning\" v-if=\"scope.row.status == 1\">保修</el-tag>\r\n              <el-tag type=\"danger\" v-if=\"scope.row.status == 2\">损坏</el-tag>\r\n            </template>\r\n          </el-table-column>\r\n          <el-table-column prop=\"rentstatus\" label=\"租用状态\" width=\"width\" align=\"center\">\r\n            <template slot-scope=\"scope\">\r\n              <el-tag type=\"success\" v-if=\"scope.row.rentstatus == 0\" >未出租</el-tag>\r\n              <el-tag type=\"warning\" v-if=\"scope.row.rentstatus == 1\">已出租</el-tag>\r\n            </template>\r\n          </el-table-column>\r\n          <el-table-column prop=\"compensation\" label=\"损失赔偿/件\" width=\"width\" align=\"center\">\r\n          </el-table-column>\r\n        </el-table>\r\n      </div>\r\n    </el-card>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: 'Equery',\r\n\r\n  data() {\r\n    return {\r\n      search:'',\r\n      equiptype:[],\r\n      select:'',\r\n      showList:[],\r\n      equipList:[]\r\n    }\r\n  },\r\n\r\n  mounted() {\r\n    // 获取全部器材\r\n    this.getAllEquipInfo()\r\n  },\r\n\r\n  methods: {\r\n    // 获取全部器材信息\r\n    async getAllEquipInfo(){\r\n      const results = await this.$store.dispatch('getAllEquipInfo')\r\n      if(results.code == 200){\r\n        this.equipList = results.data\r\n        this.equipList.forEach(item => {\r\n            this.equiptype.push(item.type)\r\n        });\r\n        // 类型去重\r\n        this.equiptype = [...new Set(this.equiptype)]\r\n        this.showList = this.equipList\r\n      }else{\r\n        this.$notify.errpr({\r\n          title:'数据获取失败'\r\n        })\r\n      }\r\n    },\r\n\r\n    change(){\r\n    if(this.select == ''){\r\n      this.showList = this.equipList\r\n    }else{\r\n      this.showList = []\r\n      this.equipList.forEach(item => {\r\n        if(this.select == item.type){\r\n          this.showList.push(item)\r\n        }\r\n      })\r\n    }\r\n  }\r\n  },\r\n\r\n  \r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n.input{\r\n  width:150px;\r\n  position: relative;\r\n  left: 1000px;\r\n}\r\n.btn{\r\n  position: relative;\r\n  left: 1020px;\r\n}\r\n</style>\r\n"]}]}