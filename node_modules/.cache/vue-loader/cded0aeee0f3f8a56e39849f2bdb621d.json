:value=\"item.value\">\r\n                    </el-option>\r\n                </el-select>\r\n                <el-input style=\"display: inline-block;width:180px;margin-left:-1px;border-radius:25px 25px 0px 0px\"></el-input>\r\n                <el-button type=\"primary\" style=\"margin-left: 50px\" @click=\"searchFilter\">搜 索</el-button>\r\n            </div>\r\n            <div>\r\n                <el-table :data=\"myApply\" style=\"width: 100%;margin:10px\">\r\n                    <el-table-column type=\"index\" label=\"序号\" width=\"80\" align=\"center\">\r\n                    </el-table-column>\r\n                    <el-table-column prop=\"gamename\" label=\"活动名称\" width=\"width\" align=\"center\">\r\n                    </el-table-column>\r\n                    <el-table-column prop=\"gametype\" label=\"活动类型\" width=\"width\" align=\"center\">\r\n                    </el-table-column>\r\n                    <el-table-column prop=\"gametime\" label=\"活动时间\" width=\"width\" align=\"center\">\r\n                    </el-table-column>\r\n                    <el-table-column prop=\"status\" label=\"审批状态\" width=\"width\" align=\"center\">\r\n                        <template slot-scope=\"scope\">\r\n                            <el-tag type=\"info\" v-if=\"scope.row.status == '审核中'\">审核中</el-tag>\r\n                            <el-tag type=\"success\" v-if=\"scope.row.status == '通过'\">通过</el-tag>\r\n                            <el-tag type=\"wraning\" v-if=\"scope.row.status == '驳回'\">驳回</el-tag>\r\n                            <el-tag type=\"warning\" v-if=\"scope.row.status == '取消申请'\">取消申请</el-tag>\r\n                        </template>\r\n                    </el-table-column>\r\n                    <el-table-column prop=\"prop\" label=\"操作\" width=\"width\" align=\"center\">\r\n                        <template slot-scope=\"{row}\">\r\n                            <el-button type=\"primary\" @click=\"exhibition(row)\">查看详情</el-button>\r\n                        </template>\r\n                    </el-table-column>\r\n                </el-table>\r\n            </div>\r\n\r\n            <el-dialog title=\"详情页\" :model=\"showlist\"  :visible.sync=\"isShow\" class=\"dialog\" >\r\n                <el-form label-position=\"rigth\" label-width=\"100px\"  class=\"form\" >\r\n                    <el-form-item label=\"活动名称:\" >\r\n                        <el-input v-model=\"showlist.gamename\" readonly=\"readonly\" class=\"input\"></el-input>\r\n                    </el-form-item>\r\n                    <el-form-item label=\"活动类型:\" >\r\n                        <el-input v-model=\"showlist.gametype\" readonly=\"readonly\" class=\"input\"></el-input>\r\n                    </el-form-item>\r\n                    <!-- <el-form-item label=\"活动ID:\" >\r\n                        <el-input v-model=\"showlist.contest_id\" readonly=\"readonly\" class=\"input\"></el-input>\r\n                    </el-form-item> -->\r\n                    <!-- <el-form-item label=\"活动地点:\" >\r\n                        <el-input v-model=\"showlist.position\" readonly=\"readonly\" class=\"input\"></el-input>\r\n                    </el-form-item> -->\r\n                    <el-form-item label=\"活动时间:\" >\r\n                        <el-input v-model=\"showlist.gametime\" readonly=\"readonly\" class=\"input\"></el-input>\r\n                    </el-form-item>\r\n                    <el-form-item label=\"活动描述:\" >\r\n                        <el-input type=\"textarea\" v-model=\"showlist.description\" readonly=\"readonly\"></el-input>\r\n                    </el-form-item>\r\n                    <el-form-item label=\"申请时间:\" >\r\n                        <el-input v-model=\"showlist.apptime\" readonly=\"readonly\" class=\"input\"></el-input>\r\n                    </el-form-item>\r\n                    <el-form-item label=\"审批号:\" v-if=\"showlist.code\" class=\"input\">\r\n                        <el-input v-model=\"showlist.code\" readonly=\"readonly\"></el-input>\r\n                    </el-form-item>\r\n                    <el-form-item label=\"申请状态\">\r\n                            <el-radio-group v-model=\"showlist.status\" size=\"medium\" disabled>\r\n                                <el-radio label=\"通过\" size=\"medium\" border class=\"radio\"></el-radio>\r\n                                <el-radio label=\"审核中\"  size=\"medium\" border class=\"radio\"></el-radio>\r\n                                <el-radio label=\"驳回\"  size=\"medium\" border class=\"radio\"></el-radio>\r\n                            </el-radio-group>\r\n                    </el-form-item>\r\n                </el-form>\r\n                <div slot=\"footer\">\r\n                    <el-button type=\"primary\" @click=\"isShow = false\">返 回</el-button>\r\n                </div>\r\n            </el-dialog>\r\n            \r\n        </el-card>\r\n    </div>\r\n    \r\n</template>\r\n\r\n<script>\r\n\r\nexport default {\r\n    name: 'Myapply',\r\n\r\n    data() {\r\n        return {\r\n            myApply:[],\r\n            isShow:false,\r\n            showlist:{},\r\n            // 模糊查询\r\n            options: [{\r\n                value: '活动',\r\n                }, {\r\n                value: '场地',\r\n                }, {\r\n                value: '器材',\r\n                }],\r\n            search:'',\r\n            select:''\r\n        };\r\n    },\r\n\r\n    mounted() {\r\n        this.getApply()\r\n    },\r\n    computed:{\r\n        \r\n    },\r\n\r\n    methods: {\r\n        // 获取用户申请数据\r\n        async getApply(){\r\n            // console.log(this.$store)\r\n            const uid = this.$store.state.user.uid\r\n            // console.log(uid)\r\n            let result = await this.$store.dispatch('user/getMyapply', uid)\r\n            // console.log(result)\r\n            if(result.message){\r\n                this.$message.error(result.message)\r\n            }else{\r\n                this.myApply = result\r\n                this.myApply.forEach(item => {\r\n                    item.gametime = this.$moment(item.gametime).format('YYYY-MM-DD HH:mm:ss')\r\n                    item.apptime = this.$moment(item.apptime).format('YYYY-MM-DD HH:mm:ss')\r\n                    switch (item.status) {\r\n                        case '0':\r\n                            item.status = '审核中'\r\n                            break;\r\n                        case '1':\r\n                            item.status = '通过'\r\n                            break;\r\n                        case '2':\r\n                            item.status = '驳回'\r\n                            break;\r\n                        case '3':\r\n                            item.status = '取消申请'\r\n                            break;\r\n                    }\r\n                });\r\n            }\r\n        },\r\n        // 展示指定申请的详情\r\n        exhibition(row){\r\n            this.showlist = row\r\n            this.isShow = true  \r\n            // console.log(row)\r\n        },\r\n        // 模糊查询\r\n        searchFilter(){\r\n            alert('该功能待开发')\r\n            // this.myApply = this.myApply.filter(item => {\r\n            //     if (item.contest_name == this.select || item.position == this.select || item.app_time == this.select || item.app_status == this.select) {\r\n                    \r\n            //     }\r\n            // })\r\n        }\r\n    },\r\n};\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n.form{\r\n    width: 450px;\r\n    position: relative;\r\n    left: 50%;\r\n    margin-left: -225px;\r\n    top:40%;\r\n}\r\n.search{\r\n    margin-top: 20px;\r\n}\r\n.radio{\r\n    background-color: aqua;\r\n}\r\n</style>"]}]}