:default-sort=\"{ prop: 'gametime', order: 'ascending' }\"\r\n          stripe\r\n          \r\n        >\r\n          <el-table-column\r\n            prop=\"gid\"\r\n            sortable\r\n            label=\"赛事编号\"\r\n            width=\"width\"\r\n            align=\"center\"\r\n          >\r\n          </el-table-column>\r\n          <el-table-column\r\n            prop=\"gametype\"\r\n            label=\"赛事类型\"\r\n            width=\"width\"\r\n            align=\"center\"\r\n          >\r\n          </el-table-column>\r\n          <el-table-column\r\n            prop=\"gamename\"\r\n            label=\"赛事名称\"\r\n            width=\"width\"\r\n            align=\"center\"\r\n          >\r\n          </el-table-column>\r\n          <el-table-column\r\n            prop=\"class\"\r\n            label=\"比赛队伍\"\r\n            width=\"width\"\r\n            align=\"center\"\r\n          >\r\n          </el-table-column>\r\n          <el-table-column\r\n            prop=\"gametime\"\r\n            sortable\r\n            label=\"比赛时间\"\r\n            width=\"width\"\r\n            align=\"center\"\r\n          >\r\n          </el-table-column>\r\n          <el-table-column\r\n            prop=\"position\"\r\n            label=\"比赛地点\"\r\n            width=\"width\"\r\n            align=\"center\"\r\n          >\r\n          </el-table-column>\r\n        </el-table>\r\n      </div>\r\n      <pagination\r\n        :currentPage=\"currentPage\"\r\n        :total=\"total\"\r\n        :limit=\"limit\"\r\n        :small=\"true\"\r\n        @handleCurrentChange=\"handleCurrentChange\"\r\n        @handleSizeChange=\"handleSizeChange\"\r\n      />\r\n    </el-card>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"Gquery\",\r\n\r\n  data() {\r\n    return {\r\n      value: \"\",\r\n      gameType: [],\r\n      showList: [],\r\n      gameList: [],\r\n      currentPage: 1,\r\n      limit: 5,\r\n      total: 0,\r\n    };\r\n  },\r\n\r\n  mounted() {\r\n    this.getGameList();\r\n  },\r\n\r\n  methods: {\r\n    // 获取全部赛事\r\n    async getGameList() {\r\n      const results = await this.$store.dispatch(\"getGameList\");\r\n      if (results.code == 200) {\r\n        this.gameList = [];\r\n        this.gameList = results.data;\r\n        this.gameList.forEach((item) => {\r\n          item.gametime = this.$moment(item.gametime).format(\r\n            \"YYYY-MM-DD HH:mm:ss\"\r\n          );\r\n          this.gameType.push(item.gametype);\r\n        });\r\n        this.gameType = [...new Set(this.gameType)]\r\n        console.log(this.gameType)\r\n        this.showList = [];\r\n        this.showList = this.gameList.slice(\r\n          (this.currentPage - 1) * this.limit,\r\n          this.limit * this.currentPage\r\n        );\r\n        this.total = this.gameList.length;\r\n      } else {\r\n        this.$notify.error({\r\n          title: \"失败\",\r\n          message: results.message,\r\n        });\r\n      }\r\n      // console.log(this.gameList)\r\n      // console.log(this.gameType)\r\n    },\r\n\r\n    //按类型展示\r\n    change() {\r\n      if (this.value == \"\") {\r\n        this.showList = this.gameList;\r\n      } else {\r\n        this.showList = [];\r\n        this.gameList.forEach((item) => {\r\n          if (item.gametype == this.value) {\r\n            this.showList.push(item);\r\n          }\r\n        });\r\n      }\r\n    },\r\n\r\n    // 分页器\r\n    handleCurrentChange(val) {\r\n      this.currentPage = val;\r\n      this.getGameList();\r\n    },\r\n    handleSizeChange(val) {\r\n      this.limit = val;\r\n      this.currentPage = 1;\r\n      this.getGameList();\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.el-table__body /deep/ .el-table__row{\r\n  height: 40px!important;\r\n}\r\n</style>\r\n"]}]}